<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- ============================================================== -->
<!-- ================可直接使用Base配置文件中定义的节点！================ -->
<!-- ============================================================== -->
<mapper namespace="com.lawfirm.lawfirmserver.lawyer.dao.LawyerSpecialtyRelationDao">
    <!-- 请在下方添加自定义配置-->
    <!-- 按主键List查询多条记录 -->
    <select id="selectBatchByLawyerId" resultMap="BaseResultMap" parameterType="map">
        select
        <include refid="Base_Column_List"/>
        from lawyer_specialty_relations
        where lawyerId = #{lawyerId}
    </select>


</mapper>