<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- ============================================================== -->
<!-- ================可直接使用Base配置文件中定义的节点！================ -->
<!-- ============================================================== -->
<mapper namespace="com.lawfirm.lawfirmserver.lawyer.dao.LawyerDao">
    <!-- 请在下方添加自定义配置-->

    <!-- 插入一条记录(为空的字段不操作) -->
    <insert id="insertSelectiveAndBackId" parameterType="com.lawfirm.lawfirmserver.lawyer.po.Lawyer">
        <selectKey keyProperty="id" resultType="Long" order="AFTER">
            SELECT LAST_INSERT_ID() AS value
        </selectKey>
        insert into lawyer
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="lawyerName != null">
                lawyerName,
            </if>
            <if test="lawFirm != null">
                lawFirm,
            </if>
            <if test="lawyerLicenseNumber != null">
                lawyerLicenseNumber,
            </if>
            <if test="provinceCode != null">
                provinceCode,
            </if>
            <if test="cityCode != null">
                cityCode,
            </if>
            <if test="districtCode != null">
                districtCode,
            </if>
            <if test="lawyerIntroduction != null">
                lawyerIntroduction,
            </if>
            <if test="lawyerDetails != null">
                lawyerDetails,
            </if>
            <if test="isValidFlag != null">
                isValidFlag,
            </if>
            operateTimeForHis,
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id},
            </if>
            <if test="lawyerName != null">
                #{lawyerName},
            </if>
            <if test="lawFirm != null">
                #{lawFirm},
            </if>
            <if test="lawyerLicenseNumber != null">
                #{lawyerLicenseNumber},
            </if>
            <if test="provinceCode != null">
                #{provinceCode},
            </if>
            <if test="cityCode != null">
                #{cityCode},
            </if>
            <if test="districtCode != null">
                #{districtCode},
            </if>
            <if test="lawyerIntroduction != null">
                #{lawyerIntroduction},
            </if>
            <if test="lawyerDetails != null">
                #{lawyerDetails},
            </if>
            <if test="isValidFlag != null">
                #{isValidFlag},
            </if>
            now(),
        </trim>
    </insert>

</mapper>