<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lawfirm.lawfirmserver.login.dao.LoginLogMapper">

    <insert id="insertLoginLog" parameterType="com.lawfirm.lawfirmserver.login.po.LoginLog" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO login_log (
            user_id,
            phone,
            login_type,
            status,
            ip,
            device,
            remark,
            create_time
        ) VALUES (
                     #{userId},
                     #{phone},
                     #{loginType},
                     #{status},
                     #{ip},
                     #{device},
                     #{remark},
                     #{createTime}
                 )
    </insert>

</mapper>