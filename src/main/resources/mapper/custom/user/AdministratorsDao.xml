<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- ============================================================== -->
<!-- ================可直接使用Base配置文件中定义的节点！================ -->
<!-- ============================================================== -->
<mapper namespace="com.lawfirm.lawfirmserver.user.dao.AdministratorsDao">
    <!-- 请在下方添加自定义配置-->

    <!-- 插入一条记录(为空的字段不操作) -->
    <insert id="insertSelectiveAndBack" parameterType="com.lawfirm.lawfirmserver.user.po.Administrators">
        <selectKey keyProperty="id" resultType="Long" order="AFTER">
            SELECT LAST_INSERT_ID() AS value
        </selectKey>
        insert into administrators
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="adminRole != null">
                admin_role,
            </if>
            <if test="adminLevel != null">
                admin_level,
            </if>
            <if test="parentAdminId != null">
                parent_admin_id,
            </if>
            <if test="isValidFlag != null">
                is_valid_flag,
            </if>
            operate_time_for_his,
        </trim>
        values
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id},
            </if>
            <if test="adminRole != null">
                #{adminRole},
            </if>
            <if test="adminLevel != null">
                #{adminLevel},
            </if>
            <if test="parentAdminId != null">
                #{parentAdminId},
            </if>
            <if test="isValidFlag != null">
                #{isValidFlag},
            </if>
            now(),
        </trim>
    </insert>

</mapper>